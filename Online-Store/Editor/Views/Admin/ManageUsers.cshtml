@model IEnumerable<Editor.Models.AdminUserModel>

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Change role</title>
    <script src="~/Scripts/jquery-2.2.3.js"></script>
    <script src="~/Scripts/jquery-2.2.3.min.js"></script>
</head>
<body>

    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            if (!User.Identity.Name.Equals(item.Email))
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.ActionLink("Change role", "UsersEdit", new { id = item.UserId }) |
                        @Html.ActionLink("Details", "UsersDetails", new { id = item.UserId }) |
                        @Html.ActionLink("Delete", "UsersDelete", new { id = item.UserId })

                    </td>
                </tr>
            }

        }

    </table>
</body>
</html>
